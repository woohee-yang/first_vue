{"ast":null,"code":"import { eventBus } from './lib/eventBus';\nexport default {\n  name: \"CellContents\",\n  props: {\n    pos: Object\n  },\n  data() {\n    return {\n      inContents: null,\n      save: false,\n      x: '',\n      y: '',\n      flag_saved: false\n    };\n  },\n  methods: {\n    updatePos() {\n      if (this.pos.x > -1 && this.pos.y > -1) {\n        this.x = this.pos.x;\n        this.y = this.pos.y;\n        return true;\n      } else return false;\n    },\n    saveData() {\n      this.save = true;\n    },\n    checkSave() {\n      console.log(\"checkSave : \", this.inContents, this.flag_saved);\n      let tmp = this.flag_saved && this.inContents;\n      return tmp;\n    }\n  },\n  watch: {\n    inContents: function () {\n      console.log(\"save : \", this.save);\n      console.log(this.inContents);\n    },\n    save: function () {\n      if (this.save) {\n        console.log('save start!');\n        eventBus.$emit('saveInputX', this.x);\n        eventBus.$emit('saveInputY', this.y);\n        eventBus.$emit('saveInputText', this.inContents);\n        this.flag_saved = true;\n        this.save = false;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAoBA;AACA;EACAA;EACAC;IACAC;EACA;EAEAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EAEAC;IACAC;MACA;QACA;QACA;QACA;MACA,OAEA;IACA;IACAC;MACA;IACA;IACAC;MACAC;MACA;MACA;IACA;EACA;EACAC;IACAV;MACAS;MACAA;IACA;IAEAR;MACA;QACAQ;QAEAE;QACAA;QACAA;QAGA;QACA;MACA;IACA;EACA;AACA","names":["name","props","pos","data","inContents","save","x","y","flag_saved","methods","updatePos","saveData","checkSave","console","watch","eventBus"],"sourceRoot":"src/components","sources":["CellContents.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h3 style=\"margin-left:5%\">2. Input inContents</h3>\r\n\r\n    <span v-if=\"updatePos()\">\r\n                <p style=\"margin-left:5%\">input column : {{x }}, input row : {{y}}</p>\r\n                <input type=\"text\" v-model=\"inContents\" style=\"margin-left:5%\" placeholder=\"input inContents\"/>\r\n                <button style=\"margin-left:10px\" @click=\"saveData\">save</button>\r\n                <span v-if=\"checkSave()\">\r\n                    <p style=\"margin-left:5%\">input inContents: {{ inContents }}</p>\r\n                </span>\r\n        </span>\r\n\r\n    <span v-else>\r\n            <p style=\"margin-left:5%\"> input current cell position first! </p>\r\n        </span>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {eventBus} from './lib/eventBus';\r\nexport default {\r\n  name: \"CellContents\",\r\n  props: {\r\n    pos: Object\r\n  },\r\n\r\n  data(){\r\n    return {\r\n      inContents: null,\r\n      save: false,\r\n      x: '',\r\n      y: '',\r\n      flag_saved: false,\r\n    }\r\n  },\r\n\r\n  methods: {\r\n    updatePos(){\r\n      if(this.pos.x > -1 && this.pos.y > -1){\r\n        this.x = this.pos.x;\r\n        this.y = this.pos.y;\r\n        return true;\r\n      }\r\n      else\r\n        return false;\r\n    },\r\n    saveData(){\r\n      this.save = true\r\n    },\r\n    checkSave(){\r\n      console.log(\"checkSave : \", this.inContents, this.flag_saved)\r\n      let tmp = this.flag_saved && this.inContents\r\n      return tmp\r\n    }\r\n  },\r\n  watch: {\r\n    inContents:function(){\r\n      console.log(\"save : \", this.save)\r\n      console.log(this.inContents)\r\n    },\r\n\r\n    save:function(){\r\n      if(this.save){\r\n        console.log('save start!')\r\n\r\n        eventBus.$emit('saveInputX', this.x)\r\n        eventBus.$emit('saveInputY', this.y)\r\n        eventBus.$emit('saveInputText', this.inContents)\r\n\r\n\r\n        this.flag_saved = true\r\n        this.save = false\r\n      }\r\n    }\r\n  },\r\n}\r\n</script>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}